<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="tempranillo.entidades.Media" table="public.tmedia">
      <id column="idmedia" name="idmedia">
      <generator class="identity"/>
    </id>
    <property column="rutaavatar" name="rutaavatar" type="string"/>
    <property column="tipo" name="tipo" type="string"/>
    <property column="idvino" name="idvino" type="integer"/>
    
  </class>
</hibernate-mapping>
