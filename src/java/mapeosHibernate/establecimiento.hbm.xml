<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="tempranillo.entidades.Establecimiento" table="public.Testablecimiento">
    <id column="idestablecimiento" name="idestablecimiento">
      <generator class="identity"/>
    </id>
    <property column="nombre" name="nombre" type="string"/>
    <property column="direccion" name="direccion" type="string"/>
    <property column="web" name="web" type="string"/>
    <property column="email" name="email" type="string"/>
    <property column="telefono" name="telefono" type="string"/>
    <property column="posiciongps" name="posiciongps" type="string"/>
    <property column="tipo" name="tipo" type="string"/>
  </class>
</hibernate-mapping>
