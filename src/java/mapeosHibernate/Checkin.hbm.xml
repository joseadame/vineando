<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="tempranillo.entidades.Checkin" table="public.Tcheckins">
    <id column="idcheckin" name="idcheckin">
      <generator class="identity"/>
    </id>
    <property column="idvino" name="idvino" type="integer"/>
    <property column="idusuario" name="idusuario" type="integer"/>
    <property column="fecha" name="fecha" type="date"/>
    <property column="comentario" name="comentario" type="string"/>
    <property column="nota" name="nota" type="double"/>
    <property column="relacion" name="relacion" type="string"/>
    <property column="ciudad" name="ciudad" type="string"/>
    <property column="establecimiento" name="establecimiento" type="string"/>
  </class>
</hibernate-mapping>
