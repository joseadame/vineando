<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="tempranillo.entidades.Votacion" table="public.Tvotaciones">
    <id column="idvotacion" name="idvotacion">
      <generator class="identity"/>
    </id>
    <property column="idcomentario" name="idcomentario" type="integer"/>
    <property column="idusuario" name="idusuario" type="integer"/>
    <property column="fecha" name="fecha" type="timestamp"/>
    <property column="tipovoto" name="tipovoto" type="string"/>
    
  </class>
</hibernate-mapping>
