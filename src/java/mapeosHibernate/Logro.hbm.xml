<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="tempranillo.entidades.sistemalogros.Logro" table="public.Tlogros">
    <id column="idlogro" name="idlogro">
      <generator class="identity"/>
    </id>
    <property column="nombrelogro" name="nombre" type="string"/>
    <property column="descripcion" name="descripcion" type="string"/>
    <property column="puntuacion" name="puntuacion" type="integer"/>
  </class>
</hibernate-mapping>
