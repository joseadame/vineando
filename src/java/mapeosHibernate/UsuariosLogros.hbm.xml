<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="tempranillo.entidades.sistemalogros.UsuarioLogros" table="public.tusuarioslogros">
    <id column="id" name="id">
      <generator class="identity"/>
    </id>
    <property column="idlogro" name="idlogro" type="integer"/>
    <property column="idusuario" name="idusuario" type="integer"/>
    <property column="fecha" name="fecha" type="date"/>
  </class>
</hibernate-mapping>
