<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="tempranillo.entidades.Reporte" table="public.Treportes">
    <id column="idreporte" name="idreporte">
      <generator class="identity"/>
    </id>
    <property column="idvino" name="idvino" type="integer"/>
    <property column="idusuario" name="idusuario" type="integer"/>
    <property column="motivo" name="motivo" type="string"/>
    <property column="descripcion" name="descripcion" type="string"/>
    
    
   
  </class>
</hibernate-mapping>
